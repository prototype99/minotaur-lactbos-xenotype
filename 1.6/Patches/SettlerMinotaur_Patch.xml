      <li>Equal Milking</li>
      <li>Minotaur Xenotype Factions</li>
      <li>Vanilla Factions Expanded - Settlers</li>
    </mods>
    <match Class="PatchOperationAdd">
      <xpath>/Defs/FactionDef[defName="SettlerMinotaur"]/xenotypeSet/xenotypeChances</xpath>
